@model IEnumerable<FlyWith.Models.SeatingPosition>

@{
    ViewBag.Title = "Index";
}

<h2>Sting positions</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SeatingPositionID }) |
            @Html.ActionLink("Details", "Details", new { id=item.SeatingPositionID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SeatingPositionID })
        </td>
    </tr>
}

</table>
