@model IEnumerable<FlyWith.Models.YesNoAnswer>

@{
    ViewBag.Title = "Index";
}
<link rel="stylesheet" href="~/Content/main1.css" />

<h2>Yes or no answer</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.YesNoAnswerID }) |
            @Html.ActionLink("Details", "Details", new { id=item.YesNoAnswerID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.YesNoAnswerID })
        </td>
    </tr>
}

</table>
